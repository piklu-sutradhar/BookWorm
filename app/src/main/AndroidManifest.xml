<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comp3350.bookworm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Presentation.HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Presentation.LoginActivity"></activity>
        <activity android:name=".Presentation.SignupActivity" />
        <activity android:name=".Presentation.AccountActivity" />
        <activity
            android:name=".BookDescription"
            android:label="@string/title_activity_book_description"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>